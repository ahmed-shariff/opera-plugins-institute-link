{
  "manifest_version": 3,

  "developer" : {
	  "name" : "Shariff Faleel",
	  "url" : "https://shariff-faleel.com"
  },

  "name": "Rename URL to institute",
  "description": "Rename URL to institute",
  "version": "1.2",
  "background": {
	  "service_worker": "background.js"
  },

  "permissions": ["tabs", "activeTab", "downloads", "contentSettings", "scripting"],
  "action": {
	  "default_icon": "icon.png",
	  "default_title": "Redirect url to institute (UBC)"
  },

  "commands": {
	  "_execute_action": {
	    "suggested_key": {
		    "default": "Ctrl+Shift+U",
		    "mac": "Command+Shift+U"
	    },
	    "description": "Renmae url to institute"
	  }
  }
}
