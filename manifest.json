{
    "manifest_version": 2,

    "developer" : {
	"name" : "Ahmed Shariff",
	"url" : "https://github.com/ahmed-shariff"
    },
    
    "name": "Rename URL to institute",
    "description": "Rename URL to institute",
    "version": "1.0",
    "background": {
	"scripts": ["background.js"]
    },

    "permissions": ["tabs", "activeTab", "downloads", "contentSettings", "<all_urls>"],
    "browser_action": {
	"default_icon": "icon.png",
	"default_title": "rename url to institute"
    },

    "commands": {
	"rename-url": {
	    "suggested_key": {
		"default": "Ctrl+Shift+U",
		"mac": "Command+Shift+U"
	    },
	    "description": "Renmae url to institute"
	}
    }
}
